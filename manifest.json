{
    "manifest_version": 3,
    "name": "Gemini Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension to interact with Gemini.",
    "permissions": [],
    "action": {
      "default_popup": "popup.html"
    },
    
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        }
      }
    }
  }
  